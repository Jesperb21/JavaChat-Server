<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="573.0" fx:controller="Controllers.Chat.ChatController"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http:javafx.com/fxml/1">
    <children>
        <TextArea fx:id="ChatTextArea" editable="false" prefHeight="150.0" prefWidth="239.0" VBox.vgrow="ALWAYS" />
        <HBox prefHeight="27.0" prefWidth="446.0">
            <children>
                <TextField fx:id="ChatMessageArea" HBox.hgrow="ALWAYS" />
                <Button mnemonicParsing="false" fx:id="ChatSendButton" text="Send" />
            </children>
        </HBox>
    </children>
</VBox>